script/source = "extends Control


func _on_QuitButton_pressed():
	get_tree().quit()


func _on_NewGameButton_pressed():
	get_tree().change_scene(\"res://Nodes/Level.tscn\")


func _on_CreditsButton_pressed():
	get_tree().change_scene(\"res://Nodes/Interface/Menu/credits_menu.tscn\")

func _ready():
	$HTTPRequest.connect(\"request_completed\", self, \"_on_request_completed\")
	$HTTPRequest.request(\"http://localhost:3000/get\")

func _on_HTTPRequest_request_completed(result, response_code, headers, body):
	
	if response_code == 200:
		var json = JSON.parse(body.get_string_from_utf8())
		var lab = get_node(\"Menu/CenterRow/Highscores/Score_container/Label_name\")
		lab.set_text(str(json.result[\"one\"]))
		var lab2 = get_node(\"Menu/CenterRow/Highscores/Score_container2/Label_name\")
		lab2.set_text(str(json.result[\"two\"]))
		var lab3 = get_node(\"Menu/CenterRow/Highscores/Score_container3/Label_name\")
		lab3.set_text(str(json.result[\"three\"]))
		var lab4 = get_node(\"Menu/CenterRow/Highscores/Score_container4/Label_name\")
		lab4.set_text(str(json.result[\"four\"]))
		var lab5 = get_node(\"Menu/CenterRow/Highscores/Score_container5/Label_name\")
		lab5.set_text(str(json.result[\"five\"]))
	pass # Replace with function body.
